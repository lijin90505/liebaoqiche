package com.ibest.card.dto.input;

import com.ibest.framework.common.persistence.BaseInputDTO;

public class CouponRecallInputDTO extends BaseInputDTO {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String couponName;

	private String recallUserId;

	private Integer status;

	private String signId;

	public String getCouponName() {
		return couponName;
	}

	public void setCouponName(String couponName) {
		this.couponName = couponName;
	}

	public String getRecallUserId() {
		return recallUserId;
	}

	public void setRecallUserId(String recallUserId) {
		this.recallUserId = recallUserId;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getSignId() {
		return signId;
	}

	public void setSignId(String signId) {
		this.signId = signId;
	}
}
