/* 框架主题样式修改 */
.app-header {
	background: #fff;
	height: 60px;	
	overflow: hidden;
}

.linkIndexTitle{
	width:200px;
	height:60px;
	display:block;
	background:#666;
	border-bottom:1px solid #2b2e33;
	float:left;
	color:#fff;
	line-height:60px;
	text-align:center;
	font-size:18px;
}

.linkIndex{
	height:60px;
	display:block;
	float:left;
}

.linkIndex>img{
	height:36px;
	margin-top:12px;
	margin-left:20px;
}

.app-slider {
	background: #272c31;
}

.app-footer {
	background: black;
}

.app-module {
	padding: 10px 15px;
	cursor: pointer;
}

.app-module.on {
	background: blue;
	color: white;
}

.app-mobule:hover {
	background: red;
}

.perPic{
	float:right;
	height:24px;
	margin-top:18px;
	margin-right:20px;
}

.perMes{
	float:left;
	cursor:pointer;
	position:relative;
}

.perMes>i{
	float:left;
	color:#FFF;
}

.perMes>span{
	float:left;
	color:#FFF;
	line-height:24px;
	margin-left:8px;
}

.perMes:hover>i,.perMes:hover>span{
	color:#f1aa21!important;
}

.exidBtn{
	color:#ccc;
	line-height:24px;
	margin-left:20px;
	border-left:1px solid #ccc;
	padding-left:20px;
}

.exidBtn:hover{
	color:#f1aa21;
}

/*个人信息*/

.header{
    width:100%;
    height: 60px;
    background-color: #e5e9f1;
}

.person-con{
    position: relative;
}

.person-pic{
    float:right;
    height: 40px;
    padding:10px 20px;
    cursor: pointer;
    color:#666;
}

.person-pic-img{
	float:left;
	width: 36px;
    height: 36px;
    border:2px solid #d4d4d4;
    border-radius: 38px;
    background-color:#fff;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
}

.person-pic-mes{
	float:left;
	line-height:40px;
	margin:0 5px;
}

.person-pic-icon{
	float:left;
	font-size:12px;
	margin-top:11px;
}

.person-news{
	float:right;
	width: 60px;
    height: 32px;
    line-height:32px;
    margin:14px 20px;
    cursor: pointer;
}

.person-news>b{
	float:left;
	margin-top:5px;
	color:#264dbe;
	font-size:20px;
}

.person-news>span{
	float:left;
	color:#666;
	font-size:12px;
	margin-left:5px;
}

.person-mes{
    position: absolute;
    top: 56px;
    right: 2px;
    z-index: 1000;
    width: 220px;
    height: 340px;
    background: rgba(75,75,75,.9);
    display:none;
}

.person-mes.showLog{
	display:block!important;
}

.headPortrait{
    width:100%;
    height:135px;
    padding-top:20px;
}

.headPortrait>i {
    display: block;
    margin:0 auto;
    width: 80px;
    height: 80px;
    border:2px solid #ccc;
    border-radius: 80px;
    text-align: center;
    line-height: 80px;
    background-color: #fff;
    background-position: center center;
    background-size: contain;
}

.headPortrait-mes{
    padding-top:10px;
}

.reMessage{
	margin-top:15px;
	text-align:center;
	line-height:20px;
}

.reMessage>a{
	border:1px solid #fff;
	color:#fff;
	padding:5px 10px;
	border-radius:5px;
	margin:0 10px;
}

.reMessage>a:hover{
	color:#407de3;
	border:1px solid #407de3;
}

.pic-left-mes:after {
    content: "";
    display: inline-block;
    width: 100%;
    overflow: hidden;
    height: 0;
}

.headPortrait-mes>p {
    width: 180px;
    float: left;
    margin-left: 30px;
}

.headPortrait-mes>p>span {
    float: left;
    height: 25px;
    line-height: 25px;
    font-size: 12px;
    color:#ccc;
}

.pic-right-mes{
    width:110px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    margin-left: 5px;
    color:#fff!important;
}

.pic-left-mes {
    width: 55px;
    color: #ccc;
    font-size: 12px;
    line-height: 25px;
    font-weight: normal;
    display: inline-block;
    text-align: justify;
    vertical-align: middle;
    margin-right: 5px;
}


