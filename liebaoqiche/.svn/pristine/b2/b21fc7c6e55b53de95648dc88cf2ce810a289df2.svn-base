.exitBtn{
	width: 200px;
	height: 30px;
    background-color:#fff;
    color: #407de3;
    border: none;
    outline: none;
    margin:10px;
    border-radius: 3px;
    cursor: pointer;
}

.exitBtn:hover{
	background-color:#f6f6f6;
}

