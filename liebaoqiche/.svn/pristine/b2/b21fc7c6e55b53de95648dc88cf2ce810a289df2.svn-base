.slider-list{
	line-height:46px;
	cursor:pointer;
	color:#f1f1f1;
	height:46px;
	overflow:hidden;
}

.slider-list.active{
	height:auto!important;
}

.slider-first{
	line-height:46px;
	padding-left:10px;
	position:relative;
}

.slider-first:hover{
	background:#f6f6f6;
	color:#264dbe;
}


.slider-first>span{
	display:inline-block;
	width:140px;
}

.sliderIcon{
	float: left;
    width: 30px;
    height: 46px;
    display: -webkit-flex;
    display: flex;
    justify-content: center;
    align-items: center;
}

.sm_slider{
	padding-left:25px;
	background:#1f252b;
	color:#999;
}

.sm_slider>i{
	color:#999;
	width:20px;
	text-align: center;
}

.sm_slider:hover{
	color:#fff;
}

.sm_slider:hover>i{
	color:#fff;
}

.sm_slider.on{
	color:#264dbe;
	background:#F6F6F6;
}

.sm_slider.on>i{
	color:#fff;
}

.rightDown{
	position:absolute;
	top:12px;
	transition: -webkit-transform 0.5s ease-out;
	-webkit-transition: -webkit-transform 0.5s ease-out;
    -moz-transition: -moz-transform 0.5s ease-out;
    -o-transition: -o-transform 0.5s ease-out;
    -ms-transition: -ms-transform 0.5s ease-out;
}

.slider-list.active .rightDown{
	transform: rotateZ(90deg);
	webkit-transform: rotateZ(90deg);
    -moz-transform: rotateZ(90deg);
    -o-transform: rotateZ(90deg);
    -ms-transform: rotateZ(90deg);
}


/* tab标签页 */
.tabs-header{
	background:#fff!important;
	padding:0!important;
	border-top: 5px solid #f6f6f6;
}

.tabs{
	height:36px!important;
	border-color:#264dbe;
	border-width: 0 0 2px 0;
}

.tabs li{
	height:36px!important;
	margin:0!important;
}

.tabs li a.tabs-inner{
	height: 36px!important;
    line-height: 36px!important;
}

.tabs li a.tabs-inner{
	border-radius:0;
	border:none;
	border-right:1px solid #d9d9d9;	
    border-bottom: none;
    background:#fff!important;
    padding: 0 25px!important;
}

.tabs li.tabs-selected{
	background:#264dbe!important;
}

.tabs li.tabs-selected a.tabs-inner{
	border-bottom: none!important;
	background:#264dbe!important;
	color:#fff;
}

.tabs li.tabs-selected .tabs-close{
	color:#fff;
}


.btnLink{
	padding-top:50px;
}
    	
.btnLink>a{
	display:block;
	margin:20px auto;
	width:40px;
	height:24px;
	padding-top:6px!important;
}

/** 详情页面 table 样式 */
.detail-table tr{
	font-size:12px;
}

.detail-table tr td:nth-child(1),.detail-table tr td:nth-child(3){
	padding: 10px;
	background: #f7f7f7;
	text-align: right;
	width:15%;
	color: #3c3c3c;
}
.detail-table tr td:nth-child(2),.detail-table tr td:nth-child(4){
	padding-left: 5px;
	text-align: left;
	width:35%;
	color: #c3c3c3;
}

.remarks{
	word-wrap:break-word;
	word-break:break-all;
}

.personPic{
	width:90px;
	height:90px;
	margin: auto;
}

/* 表格分块标题样式 */
.chapter {
	height: 30px !important; 
	line-height: 30px !important; 
	padding: 5px 10px !important; 
	border-left: 5px solid #264dbe !important;
	font-weight: bold !important;
	color: #3c3c3c !important;
	text-align: left !important;
	background: white !important;
	font-size: 16px !important;
	border-right: none !important;
	border-bottom: none !important;
}

table tr td div,table tr td p{
	word-break: break-word;
}



/* 错误页面 */

.error_page{
	width:300px;
	margin:40px auto 0;
}

.error_page img{
	width:100%;
}

.error_page p{
	text-align:center;
	color:#666;
	margin-top:30px;
}

