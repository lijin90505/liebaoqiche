/* 加载中 */
.pageLoading{
	position:fixed;
	top:0;
	left:0;
	bottom:0;
	right:0;
	width:100%;
	height:100%;
	background:transparent;
	z-index:1000000;
	display:none;
}

.pageLoading_con{
	position:absolute;
	top:0;
	left:0;
	bottom:0;
	right:0;
	width:200px;
	height:80px;
	text-align:center;
	margin:auto;
	background:#fff;
	border-radius:6px;
}

.pageLoading_con>img{
	width:50px;
}

.pageLoading_con>p{
	color:#666;
}
