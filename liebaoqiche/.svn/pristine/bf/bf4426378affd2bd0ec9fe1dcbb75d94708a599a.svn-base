package com.ibest.card.dto.input;

import com.ibest.framework.common.persistence.BaseInputDTO;

public class CardOperationInputDTO extends BaseInputDTO {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String cardId;

	private String cardName;

	private String grantOrRecall;

	public String getCardId() {
		return cardId;
	}

	public void setCardId(String cardId) {
		this.cardId = cardId;
	}

	public String getCardName() {
		return cardName;
	}

	public void setCardName(String cardName) {
		this.cardName = cardName;
	}

	public String getGrantOrRecall() {
		return grantOrRecall;
	}

	public void setGrantOrRecall(String grantOrRecall) {
		this.grantOrRecall = grantOrRecall;
	}
}
