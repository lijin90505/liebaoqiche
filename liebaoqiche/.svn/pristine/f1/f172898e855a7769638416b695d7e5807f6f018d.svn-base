<%@ page contentType="text/html;charset=UTF-8"%>
<%@ include file="/WEB-INF/views/include/taglib.jsp"%>
<!DOCTYPE>
<html>
<head>
	<title>PaySystemOrder</title>
	<%@ include file="/WEB-INF/views/include/head.jsp"%>
</head>
<body>
	<%@ include file="/WEB-INF/views/include/loading.jsp"%>
	<div class="easyui-layout" data-options="fit:true">
		<div data-options="region:'center'">
    		<table style="width: 100%;">
    			<tbody>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						主键
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.id}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						交易时间
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.tradeTime}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						appid
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.appid}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						商户号
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.merchantNo}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						子商户号
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.subMerchantNo}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						设备号
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.deviceNo}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						支付系统流水号
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.transactionId}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						商户订单号
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.orderId}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						用户标识
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.openid}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						交易类型
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.tradeType}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						交易状态
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.tradeState}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						付款银行
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.bankType}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						货币种类
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.feeType}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						总金额
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.totalFee}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						代金券或立减优惠金额
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.couponFee}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						商品名称
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.productName}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						商户数据包
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.attach}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						手续费
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.serviceCharge}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						费率
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.rate}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						创建人
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.creater}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						创建时间
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.createTime}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						修改人
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.modifier}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						修改时间
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.modifyTime}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						备注
    					</td>
    					<td style="width: 50%; padding: 5px;">
    						${paySystemOrder.remarks}
    					</td>
    				</tr>
    				<tr>
    					<td style="width: 50%; padding: 5px;">
    						<label class="label-top">备注:</label>
    						<input class="easyui-textbox" name="remarks" style="width:99.5%; height:80px;" data-options="multiline:true">
    					</td>
    				</tr>
    			</tbody>
    		</table>
		</div>
		<div data-options="region:'south',border:false" style="text-align:left; padding:5px;">
            <a id="btnCancel_PaySystemOrder" href="javascript:void(0)" class="easyui-linkbutton button-warning" style="width: 80px;"><i class="fa fa-remove fa-lg"></i>&nbsp;&nbsp;取消</a>
        </div>
   	</div>
    
    <!-- js脚本，必须写在body中，tab的url加载模式只会异步加载body中的内容到tab中 -->
	<script type="text/javascript">
		require(['jquery','common'], function($){
			// 给保存按钮添加点击事件
			$("#btnCancel_PaySystemOrder").on('click', function(){
				history.go(-1);
			});
		});
	</script>
</body>
</html>